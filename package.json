{
  "name": "app2",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "ts-bin": "ts-node -r tsconfig-paths/register",
    "ts-bin:dev": "cross-env NODE_ENV=development nodemon src/main.ts",
    "ts-bin:prod": "tsc -P tsconfig.json && tsc-alias",
    "cleanup": "rimraf dist",
    "start": "npm run mig:latest && node dist/main.js",
    "build": "npm run cleanup && npm run ts-bin:prod",
    "dev": "DEBUG=* npm run ts-bin:dev",
    "kmig": "knex --knexfile knexfile.ts migrate:latest",
    "kroll": "knex --knexfile knexfile.ts migrate:rollback",
    "kseed": "knex --knexfile knexfile.ts seed:run",
    "klist": "knex --knexfile knexfile.ts migrate:list",
    "kmake": "knex --knexfile knexfile.ts migrate:make"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "argon2": "^0.30.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jose": "^4.13.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-transform": "^0.0.4",
    "knex": "^2.4.2",
    "moment": "^2.29.4",
    "pg": "^8.10.0",
    "short-uuid": "^4.2.2",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/faker": "^6.6.9",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/knex": "^0.16.1",
    "@types/node": "^18.15.8",
    "@types/nodemon": "^1.19.2",
    "@types/pg": "^8.6.6",
    "@types/prettier": "^2.7.2",
    "@types/rimraf": "^4.0.5",
    "@types/validator": "^13.7.14",
    "@types/yup": "^0.32.0",
    "body-parse": "^0.1.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.22",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.4",
    "tsconfig-paths": "^4.1.2",
    "tslib": "^2.5.0",
    "typescript": "^4.7.3"
  }
}
